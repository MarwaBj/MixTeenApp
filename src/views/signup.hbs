<div class="form-container">
  <section id="sp_form">
    <form>
      <div class="input_form">
        <div class="log_ico">
          <i class="material-icons">perm_identity</i>
        </div>
        <div class="log_input">
          <label> اسم المستخدم</label>
          <input id="username" class="input_box" type="text" name="name" autocomplete="off" required>
        </div>
      </div>
      <div class="input_form">
        <div class="log_ico">
          <i class="material-icons">email</i>
        </div>
        <div class="log_input">
          <label> الايميل</label>
          <input id="email" class="input_box" type="email" name="name" autocomplete="off" required>
        </div>
      </div>
      <div class="input_form">
        <div class="log_ico">
          <i class="material-icons">vpn_key</i>
        </div>
        <div class="log_input">
          <label>  كلمة السر</label>
          <input id="password" class="input_box" type="password" name="name" autocomplete="off" required>
        </div>
      </div>
      <div class="input_form">
        <div class="log_ico">
          <i class="material-icons">verified_user</i>
         </div>
        <div class="log_input">
          <label> تأكيد كلمة السر</label>
          <input id="cf_password" class="input_box" type="password" name="name" autocomplete="off" required>
        </div>
      </div>
      <div class="but_form">
        <div id="button_sign_in" class="button">
          <button id="signup" class="log_button" type="button" name="button"><span>  تاكيد التسجيل</span></button>
        </div>
      </div>
    </form>
  </section>
</div>
<script type="text/javascript" src="/js/signupDom.js"></script>
